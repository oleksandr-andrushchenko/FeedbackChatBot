<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="app.controller.telegram" class="App\Controller\TelegramController">
            <argument type="service" id="app.repository.telegram_bot"/>
            <argument type="service" id="app.telegram_registry"/>
            <argument type="service" id="app.telegram_update_handler"/>
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="app.telegram_site_view_response_factory"/>
            <argument type="service" id="logger"/>
            <tag name="controller.service_arguments"/>
        </service>
    </services>
</container>
