<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="app.repository.telegram_update" class="App\Repository\Telegram\TelegramUpdateRepository">
            <argument type="service" id="doctrine"/>
        </service>
        <service id="app.repository.telegram_conversation"
                 class="App\Repository\Telegram\TelegramConversationRepository">
            <argument type="service" id="doctrine"/>
        </service>
        <service id="app.repository.telegram_stopped_conversation"
                 class="App\Repository\Telegram\TelegramStoppedConversationRepository">
            <argument type="service" id="doctrine"/>
        </service>
        <service id="app.repository.telegram_payment" class="App\Repository\Telegram\TelegramPaymentRepository">
            <argument type="service" id="doctrine"/>
        </service>
        <service id="app.repository.telegram_request" class="App\Repository\Telegram\TelegramRequestRepository">
            <argument type="service" id="doctrine"/>
        </service>
        <service id="app.repository.telegram_bot" class="App\Repository\Telegram\TelegramBotRepository">
            <argument type="service" id="doctrine"/>
        </service>
        <service id="app.repository.telegram_payment_method"
                 class="App\Repository\Telegram\TelegramPaymentMethodRepository">
            <argument type="service" id="doctrine"/>
        </service>
    </services>
</container>