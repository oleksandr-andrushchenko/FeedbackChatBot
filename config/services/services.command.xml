<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="app.command_options_factory" class="App\Service\Command\CommandOptionsFactory" lazy="true"/>
        <service id="app.command_statistics_provider_factory" class="App\Service\Command\CommandStatisticsProviderFactory" lazy="true">
            <argument type="tagged_locator" tag="app.command_statistics_provider"/>
        </service>
        <service id="app.command_limits_checker" class="App\Service\Command\CommandLimitsChecker" lazy="true"/>
    </services>
</container>