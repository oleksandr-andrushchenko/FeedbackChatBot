<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="app.repository.telegram_update" class="App\Repository\Telegram\TelegramUpdateRepository">
            <argument type="service" id="doctrine"/>
        </service>
        <service id="app.repository.telegram_conversation" class="App\Repository\Telegram\TelegramConversationRepository">
            <argument type="service" id="doctrine"/>
        </service>
    </services>
</container>