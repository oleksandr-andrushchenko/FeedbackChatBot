<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="app.address_reverse_geocode" class="App\Command\Address\AddressReverseGeocodeCommand" lazy="true">
            <argument type="service" id="app.google_address_geocoder"/>
            <argument type="service" id="app.address_info_provider"/>
            <argument type="service" id="app.doctrine_dry_runner"/>
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <tag name="console.command" command="address:reverse-geocode"/>
        </service>
    </services>
</container>
