<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="app.encoder.telegram_bot_command" class="App\Serializer\Telegram\Bot\TelegramBotCommandEncoder"
                 lazy="true">
            <tag name="serializer.encoder"/>
        </service>
        <service id="app.normalizer.telegram_bot_conversation_state"
                 class="App\Serializer\Telegram\Bot\TelegramBotConversationStateNormalizer" lazy="true">
            <tag name="serializer.normalizer" priority="-970"/>
        </service>
        <service id="app.normalizer.telegram_bot_payment_method"
                 class="App\Serializer\Telegram\Bot\TelegramBotPaymentMethodNormalizer" lazy="true">
            <tag name="serializer.normalizer" priority="-967"/>
        </service>
        <service id="app.normalizer.telegram_bot_payment" class="App\Serializer\Telegram\Bot\TelegramBotPaymentNormalizer"
                 lazy="true">
            <argument type="service" id="app.normalizer.money"/>
            <tag name="serializer.normalizer" priority="-966"/>
        </service>
    </services>
</container>
