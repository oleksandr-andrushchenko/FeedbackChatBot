<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="app.repository.feedback" class="App\Repository\Feedback\FeedbackRepository">
            <argument type="service" id="doctrine"/>
        </service>
        <service id="app.repository.feedback_search" class="App\Repository\Feedback\FeedbackSearchRepository">
            <argument type="service" id="doctrine"/>
        </service>
        <service id="app.repository.feedback_search_search"
                 class="App\Repository\Feedback\FeedbackSearchSearchRepository">
            <argument type="service" id="doctrine"/>
        </service>
        <service id="app.repository.feedback_user_subscription"
                 class="App\Repository\Feedback\FeedbackUserSubscriptionRepository">
            <argument type="service" id="doctrine"/>
        </service>
    </services>
</container>